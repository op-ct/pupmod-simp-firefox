{
  "name": "simp-firefox",
  "version": "0.0.1",
  "author": "SIMP Team",
  "summary": "Manage Firefox",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-firefox",
  "project_page": "https://github.com/simp/pupmod-simp-firefox",
